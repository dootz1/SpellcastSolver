      <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="52.0" prefWidth="149.0">
         <children>
            <BorderPane prefHeight="23.0" prefWidth="149.0">
               <left>
                  <Label styleClass="text-strong" text="Max Swaps" BorderPane.alignment="CENTER" />
               </left>
               <right>
                  <Label fx:id="maxSwapsLabel" styleClass="text-strong" text="0" BorderPane.alignment="CENTER" />
               </right>
               <VBox.margin>
                  <Insets left="8.0" right="8.0" />
               </VBox.margin>
            </BorderPane>
            <Slider fx:id="maxSwapsSlider" blockIncrement="1.0" majorTickUnit="1.0" max="3.0" minorTickCount="0" showTickLabels="true" showTickMarks="true" snapToTicks="true" />
         </children>
      </VBox>
   </children>
